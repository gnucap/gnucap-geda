
include ../include/analog.v

spice
V1 n1 n0 pulse iv=0 pv=1 delay=.1m
R1 n0 0 1

.load lang_geda.so

.verilog
gschem rc.sch module
rc.sch sch1(n1 n2 n0);
list

spice
.echo dc
.print dc v(nodes) r(sch1.R1.*) c(sch1.C1.*) v(sch1.cmpnode*)
.print tran v(nodes) v(sch1.C1.c) v(sch1.R1.r)
.dc

.store tran v(n2)
.tran 0 2m .1m basic > rc_tr.out
.measure vn2 at(probe="v(n2)" x=1m)

.end
